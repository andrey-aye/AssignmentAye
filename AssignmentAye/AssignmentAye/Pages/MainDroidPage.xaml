    <pages:MasterDroidPage x:Name="MenuDroid"></pages:MasterDroidPage>
  </MasterDetailPage.Master>

  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:CarouselDroidPage></pages:CarouselDroidPage>
